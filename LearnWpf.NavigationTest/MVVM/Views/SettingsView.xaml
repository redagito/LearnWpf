<UserControl x:Class="LearnWpf.NavigationTest.MVVM.Views.SettingsView"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:LearnWpf.NavigationTest.MVVM.Views"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800">
    <Grid Background="#404040">
        <StackPanel Margin="10">
            <TextBlock Text="Settings View" FontSize="24" Foreground="LightGray" />
            <Button Content="Home" Margin="0 5 0 0" Command="{Binding NavigateHomeCommand}" />
        </StackPanel>
    </Grid>
</UserControl>
